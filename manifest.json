{
  "name": "SimpleMarks",
  "version": "0.0.1",
  "version_name": "0.0.1 beta",
  "manifest_version": 2,
  "description": "Better way to view bookmarks!",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "AngularChromeBookmarks"
  },
  "background": {
    "scripts": [
      "js/app/background.js"
    ]
  },
  "minimum_chrome_version": "18",
  "permissions": [
    "<all_urls>",
    "chrome://favicon/",
    "bookmarks",
    "tabs"
  ]
}